    "@types/window-or-global": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.4",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jsdom": "^15.1.1",
    "mocha": "^6.1.4",
    "moxios": "^0.4.0",
    "nyc": "^14.1.1",
