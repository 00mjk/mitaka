{
  "manifest_version": 2,
  "name": "Mitaka",
  "description": "Chrome Extension sends a search query to urlscan.io via the context menu",
  "version": "0.1",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["js/vendor.js", "js/background.js"]
  },
  "permissions": [
    "contextMenus"
  ]
}
